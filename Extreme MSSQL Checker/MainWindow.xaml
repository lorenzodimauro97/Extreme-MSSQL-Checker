<Window x:Class="Extreme_MSSQL_Checker.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Extreme_MSSQL_Checker"
        mc:Ignorable="d"
        Title="Extreme MSSQL Checker   LDM Inc. (AUB3L4B)" Height="320" Width="400">
    <Grid>
        <Button x:Name="Button" Content="Import Excel" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="50" Width="100" Click="ExcelImport_Click"/>
        <Button x:Name="CsvImportButton" Content="Import CSV" HorizontalAlignment="Right" Margin="270,10,10,0" VerticalAlignment="Top" Click="CSVImport_Click" Width="100" Height="50"/>
        <TextBox x:Name="OutputBox" IsReadOnly="True" HorizontalAlignment="Center" Margin="0,0,0,10" TextWrapping="Wrap" VerticalAlignment="Bottom" Width="380" Height="170" AcceptsReturn="True"
                 VerticalScrollBarVisibility="Visible" />
        <Label x:Name="ServerLabel" HorizontalAlignment="Left" Margin="4,68,0,0" VerticalAlignment="Top" Width="380"/>

    </Grid>
</Window>
